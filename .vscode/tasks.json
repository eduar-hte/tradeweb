{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++ build tests",
            "command": "/usr/bin/g++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${fileBasenameNoExtension}.cpp",
                "OrderCache.cpp",
                "-o",
                "${fileBasenameNoExtension}",
                "/usr/lib/libgtest.a",
                "/usr/lib/libgtest_main.a",
                "-lpthread",
                "-std=c++17"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}